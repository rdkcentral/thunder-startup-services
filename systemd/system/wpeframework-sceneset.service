[Unit]
Description=WPEFramework SceneSet Initialiser
Requires= wpeframework.service wpeframework-appmanager.service
After= wpeframework.service wpeframework-appmanager.service
ConditionPathExists=/opt/ai2managers
ConditionPathExists=/tmp/wpeframeworkstarted

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStartPre=-/usr/bin/ai2managers_prereqs.sh &
ExecStart=/usr/bin/PluginActivator org.rdk.SceneSet
