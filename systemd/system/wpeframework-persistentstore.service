[Unit]
Description=WPEFramework PersistentStore Initialiser
Requires=wpeframework-powermanager.service
After=wpeframework-powermanager.service securemount.service
ConditionPathExists=/tmp/wpeframeworkstarted
RequiresMountsFor=/opt/secure
[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/PluginActivator org.rdk.PersistentStore
ExecStop=/lib/rdk/deactivate_plugin.sh org.rdk.PersistentStore
