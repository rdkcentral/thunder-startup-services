# WPEFramework Migration Initialiser
# Activates the Migration plugin after framework initialization
[Unit]
Description=WPEFramework Migration Initialiser
Requires=wpeframework.service
After=wpeframework.service
ConditionPathExists=/tmp/wpeframeworkstarted
[Service]
Type=oneshot
RemainAfterExit=yes
TimeoutStartSec=30
TimeoutStopSec=10
PrivateTmp=yes
ExecStart=/usr/bin/PluginActivator org.rdk.Migration

[Install]
WantedBy=multi-user.target
