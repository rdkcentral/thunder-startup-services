[Unit]
Description=WPEFramework AppcLifecycleManager Initialiser
Requires= wpeframework.service wpeframework-lifecyclemanager.service
After= wpeframework.service wpeframework-lifecyclemanager.service
ConditionPathExists=/opt/ai2managers
ConditionPathExists=/tmp/wpeframeworkstarted

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStartPre=-/usr/bin/ai2managers_prereqs.sh &
ExecStart=/usr/bin/PluginActivator org.rdk.AppcLifecycleManager
