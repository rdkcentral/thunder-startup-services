[Unit]
Description=WPEFramework OCIContainer Plugin Initialiser
Requires=wpeframework.service iarmbusd.service dsmgr.service dobby.service
After=wpeframework.service iarmbusd.service dsmgr.service dobby.service
ConditionPathExists=/tmp/wpeframeworkstarted

[Service]
Type=oneshot
ExecStart=/usr/bin/PluginActivator org.rdk.OCIContainer
RemainAfterExit=yes
