[Unit]
Description=WPEFramework SystemAudioPlayer Initialiser
Requires=wpeframework-powermanager.service dsmgr.service wpeframework-avinput.service
After=wpeframework-powermanager.service dsmgr.service wpeframework-avinput.service
ConditionPathExists=/tmp/wpeframeworkstarted
[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/PluginActivator org.rdk.SystemAudioPlayer
