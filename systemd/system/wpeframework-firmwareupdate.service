[Unit]
Description=WPEFramework FirmwareUpdate Initialiser
Requires=wpeframework-powermanager.service wpeframework-avinput.service
After=wpeframework-powermanager.service wpeframework-avinput.service
ConditionPathExists=/tmp/wpeframeworkstarted
[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/PluginActivator org.rdk.FirmwareUpdate
