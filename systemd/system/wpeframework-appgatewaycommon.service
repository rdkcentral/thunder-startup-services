[Unit]
Description=WPEFramework AppGatewayCommon Initialiser
Requires=wpeframework.service wpeframework-appgateway.service
After=wpeframework.service wpeframework-appgateway.service
ConditionPathExists=/tmp/wpeframeworkstarted
ConditionPathExists=/opt/appgatewayenabled

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/PluginActivator org.rdk.appgatewaycommon.service
